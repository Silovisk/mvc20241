							"        'Content-Type': 'application/json'\r",
							"    },\r",
							"    body: {\r",
							"        mode: 'raw',\r",
							"        raw: JSON.stringify({\r",
							"            \"nome\": pm.variables.get(\"nome\"),\r",
							"            \"email\": pm.variables.get(\"email\")\r",
							"        })\r",
							"    }\r",
							"}, function (err, res) {\r",
							"    try {\r",
							"        if (err) {\r",
							"            console.log(\"Erro:\", err);\r",
							"        } else {\r",
							"            // Manipulação manual da resposta para remover \"null\"\r",
							"            var responseText = res.text().replace(\"null\", \"\");\r",
							"            var responseBody = JSON.parse(responseText);\r",
							"\r",
							"            console.log(\"Corpo da resposta:\", responseBody);\r",
							"            console.log(\"Token:\", responseBody.retorno);\r",
							"            pm.environment.set(\"TOKEN\", responseBody.retorno);\r",
							"        }\r",
							"    } catch (e) {\r",
							"        console.log(\"Erro:\", e);\r",
							"    }\r",
							"});\r",
							""
						],
